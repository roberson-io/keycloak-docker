# Keycloak Domain Configuration
KEYCLOAK_DOMAIN=keycloak.example.com

# Database Configuration
DB_NAME=keycloak
DB_USER=keycloak
# IMPORTANT: Generate a secure password for production
DB_PASSWORD=CHANGE_ME_SECURE_DB_PASSWORD

# Keycloak Admin Configuration
# IMPORTANT: Change these credentials for production
KEYCLOAK_ADMIN_USER=admin
# IMPORTANT: Generate a secure password for production
KEYCLOAK_ADMIN_PASSWORD=CHANGE_ME_SECURE_ADMIN_PASSWORD

# Keycloak Logging Configuration
# Options: TRACE, DEBUG, INFO, WARN, ERROR, FATAL, OFF
KC_LOG_LEVEL=INFO

# Let's Encrypt Configuration
LETSENCRYPT_EMAIL=username@example.com
